#Makefile for keyboard demo, *nix
#
CC = g++ 

LIBS = -framework OpenGL -framework Cocoa -lSDL -lSDL_image -lSDLMain -lXOR -lm

#Packages
SRCS = InterpolationManager.cpp PhotoAlbumDriver.cpp PhotoKeyboardListener.cpp PhotoAlbum.cpp

OBJS = $(SRCS:.cpp=.o)

MAIN = PhotoAlbum

# 
# The following part of the makefile is generic; it can be used to 
# build any executable just by changing the definitions above and by
# deleting dependencies appended to the file from 'make depend'
#

.PHONY: depend clean
#Change this so that the executable is put in a "bin" directory of sorts
all:		$(MAIN)
			./$(MAIN)

PhotoAlbum:	$(OBJS) 
			$(CC) $(CFLAGS) $(INCLUDES) -o $(MAIN) $(OBJS) $(LFLAGS) $(LIBS)

.c.o:
			$(CC) $(CFLAGS) $(INCLUDES) -c $<

clean:
			$(RM) $(OBJS) $(MAIN)

depend: $(SRCS)
			makedepend -fMakefile.osx &>/dev/null $(INCLUDES) $^

